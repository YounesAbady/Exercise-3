@page
@model RazorPages.Pages.Categories.EditModel
@{
    <form method="post">
        <div>
            <div class="mt-4">
                <label asp-for=Category></label>
                <input asp-for=Category>
                <span asp-validation-for="Category" class="text-danger"></span>
            </div>
            <br> <br>
            <button type="submit" class="btn btn-primary"><i class="bi bi-check">Edit</i></button>
        </div>
    </form>
}
